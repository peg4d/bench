<?xml encoding="UTF-8"?>

<!ELEMENT config (site)>
<!ATTLIST config
  xmlns CDATA #FIXED ''
  date CDATA #REQUIRED
  transient NMTOKEN #REQUIRED>

<!ELEMENT site (feature)+>
<!ATTLIST site
  xmlns CDATA #FIXED ''
